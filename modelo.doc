\par \pard\plain \s0\ql\widctlpar\ltrpar{\*\hyphen2\hyphlead2\hyphtrail2\hyphmax0}\cf1\kerning1\dbch\af11\langfe1081\dbch\af11\afs24\alang1081\loch\f5\fs24\lang1046\sl360\slmult1\ql\widctlpar\ltrpar\b0\rtlch \ltrch\loch\loch\f7

